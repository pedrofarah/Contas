    </PackageReference>
    <PackageReference Include="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.14.0" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.2.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Contas.Web.Api.Data\Contas.Web.Api.Data.csproj" />
    <ProjectReference Include="..\Contas.Web.Api.Infrastructure\Contas.Web.Api.Infrastructure.csproj" />
